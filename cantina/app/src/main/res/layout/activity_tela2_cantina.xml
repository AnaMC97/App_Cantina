<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Tela_2_Cantina">

    <Button
        android:id="@+id/sair3"
        android:layout_width="70dp"
        android:layout_height="80dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="16dp"
        android:backgroundTint="#6ECCAF"
        android:gravity="center"
        android:onClick="sair"
        android:paddingLeft="15dp"
        android:text="X"
        android:textSize="12pt"
        app:cornerRadius="70dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ajuda2" />

    <Button
        android:id="@+id/quafeira"
        android:layout_width="140dp"
        android:layout_height="120dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="80dp"
        android:backgroundTint="#8C8C8C"
        android:text="4f"
        android:textSize="60sp"
        app:layout_constraintStart_toEndOf="@+id/terfeira"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/sexfeira"
        android:layout_width="140dp"
        android:layout_height="120dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="80dp"
        android:backgroundTint="#8C8C8C"
        android:text="6f"
        android:textSize="60sp"
        app:layout_constraintStart_toEndOf="@+id/quifeira"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/segfeira"
        android:layout_width="140dp"
        android:layout_height="120dp"
        android:layout_marginStart="436dp"
        android:layout_marginTop="80dp"
        android:backgroundTint="#8C8C8C"
        android:text="2f"
        android:textSize="60sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/terfeira"
        android:layout_width="140dp"
        android:layout_height="120dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="80dp"
        android:backgroundTint="#8C8C8C"
        android:text="3f"
        android:textSize="60sp"
        app:layout_constraintStart_toEndOf="@+id/segfeira"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/quifeira"
        android:layout_width="140dp"
        android:layout_height="120dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="80dp"
        android:backgroundTint="#8C8C8C"
        android:text="5f"
        android:textSize="60sp"
        app:layout_constraintStart_toEndOf="@+id/quafeira"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/atualizarementa"
        android:layout_width="300dp"
        android:layout_height="120dp"
        android:layout_marginStart="75dp"
        android:layout_marginTop="128dp"
        android:backgroundTint="#6ECCAF"
        android:onClick="AtualizarEmenta"
        android:text="Atualizar Ementa"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/atualizaSopa" />

    <ImageButton
        android:id="@+id/atualizaSopa"
        android:layout_width="140dp"
        android:layout_height="120dp"
        android:layout_marginStart="75dp"
        android:layout_marginTop="10dp"
        android:backgroundTint="#FFDAB9"
        android:scaleType="centerInside"
        android:text="Sopa"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:srcCompat="@drawable/sopa"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/atualizaSobremesa"
        android:layout_width="140dp"
        android:layout_height="120dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="10dp"
        android:backgroundTint="#FFC0CB"
        android:scaleType="centerInside"
        android:text="Sobremesa"
        android:textSize="16sp"
        app:layout_constraintStart_toEndOf="@+id/atualizaSopa"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:srcCompat="@drawable/sobremesa"
        tools:ignore="SpeakableTextPresentCheck" />

    <Button
        android:id="@+id/Voltar"
        android:layout_width="300dp"
        android:layout_height="120dp"
        android:layout_marginStart="75dp"
        android:layout_marginTop="30dp"
        android:backgroundTint="#8C8C8C"
        android:onClick="Voltar"
        android:text="Voltar"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/atualizarementa" />

    <EditText
        android:id="@+id/ementasopasobremesa"
        android:layout_width="595dp"
        android:layout_height="110dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="48dp"
        android:background="#FFDAB9"
        android:text="Button"
        android:textColor="#000000"
        android:textSize="40sp"
        app:layout_constraintStart_toEndOf="@+id/imgsopasobremesa"
        app:layout_constraintTop_toBottomOf="@+id/segfeira" />

    <ImageButton
        android:id="@+id/imgcarne"
        android:layout_width="110dp"
        android:layout_height="110dp"
        android:layout_marginStart="444dp"
        android:layout_marginTop="12dp"
        android:background="#CCA6A6"
        android:scaleType="centerInside"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imgsopasobremesa"
        app:srcCompat="@drawable/carne"
        tools:ignore="SpeakableTextPresentCheck" />

    <EditText
        android:id="@+id/ementacarne"
        android:layout_width="595dp"
        android:layout_height="110dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="12dp"
        android:background="#CCA6A6"
        android:text="Button"
        android:textColor="#000000"
        android:textColorHint="#000000"
        android:textSize="40sp"
        app:layout_constraintStart_toEndOf="@+id/imgcarne"
        app:layout_constraintTop_toBottomOf="@+id/ementasopasobremesa" />

    <EditText
        android:id="@+id/ementaveg"
        android:layout_width="595dp"
        android:layout_height="110dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:background="#3cb371"
        android:text="Button"
        android:textColor="#000000"
        android:textColorHint="#000000"
        android:textSize="40sp"
        app:layout_constraintStart_toEndOf="@+id/imgveg"
        app:layout_constraintTop_toBottomOf="@+id/ementapeixe" />

    <ImageButton
        android:id="@+id/imgpeixe"
        android:layout_width="110dp"
        android:layout_height="110dp"
        android:layout_marginStart="444dp"
        android:layout_marginTop="12dp"
        android:background="#90BEE3"
        android:scaleType="centerInside"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imgcarne"
        app:srcCompat="@drawable/peixe"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/imgsopasobremesa"
        android:layout_width="110dp"
        android:layout_height="110dp"
        android:layout_marginStart="444dp"
        android:layout_marginTop="48dp"
        android:background="#FFDAB9"
        android:scaleType="centerInside"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/segfeira"
        app:srcCompat="@drawable/sopa"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/imgveg"
        android:layout_width="110dp"
        android:layout_height="110dp"
        android:layout_marginStart="444dp"
        android:layout_marginTop="8dp"
        android:background="#3cb371"
        android:scaleType="centerInside"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imgpeixe"
        app:srcCompat="@drawable/vegatariano"
        tools:ignore="SpeakableTextPresentCheck" />

    <EditText
        android:id="@+id/ementapeixe"
        android:layout_width="595dp"
        android:layout_height="110dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="12dp"
        android:background="#90BEE3"
        android:text="Button"
        android:textColor="#000000"
        android:textColorHint="#000000"
        android:textSize="40sp"
        app:layout_constraintStart_toEndOf="@+id/imgpeixe"
        app:layout_constraintTop_toBottomOf="@+id/ementacarne" />

    <Button
        android:id="@+id/ajuda2"
        android:layout_width="70dp"
        android:layout_height="80dp"
        android:layout_marginTop="120dp"
        android:layout_marginEnd="16dp"
        android:backgroundTint="#6ECCAF"
        android:onClick="onClick"
        android:text="\?"
        android:textAlignment="center"
        android:textSize="40dp"
        app:cornerRadius="70dp"
        app:iconPadding="5dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="300dp"
        android:layout_height="80dp"
        android:layout_marginStart="75dp"
        android:layout_marginTop="110dp"
        android:background="#8C8C8C"
        android:gravity="center"
        android:text="ALTERAR:"
        android:textColor="@color/white"
        android:textSize="40dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textoajudar2"
        android:layout_width="720dp"
        android:layout_height="500dp"
        android:gravity="center"
        android:text="@string/AjudaEmenta"
        android:textSize="17pt"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.792"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.666" />

</androidx.constraintlayout.widget.ConstraintLayout>